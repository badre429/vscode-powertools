{
  "snippet_item": {
    "prefix": "snippet_item",
    "description": "snipet json value",
    "body": [
      "\"${1:snippet}\": {",
      "  \"prefix\": \"$1\",",
      "  \"description\": \"$2\",",
      "  \"body\": [$3]",
      "},"
    ]
  },
  "csPosgres": {
    "prefix": "csPosgres",
    "description": "",
    "body": [
      "\"${1:Default}\": \"Server=${2:localhost};Port=${3:5432};Database=${4:DBNAME};Userid=${5:USERNAME};Password=${6:PASSWORD};${7:sslmode=Disable;Trust Server Certificate=true;}\",$0"
    ]
  },
  "csSqlite": {
    "prefix": "csSqlite",
    "description": "",
    "body": [
      "\"${1:Default}\": \"DataSource={2:app.db};${3:Cache=Shared};\",$0"
    ]
  },
  "csSqlserver": {
    "prefix": "csSqlserver",
    "description": "",
    "body": [
      "\"${1:Default}\": \"Server=${2:localhost};Database=${3:DBNAME};User ID=${4:USERNAME};Password=${5:PASSWORD};${6:Trusted_Connection=True;MultipleActiveResultSets=true;}\",$0"
    ]
  },
  "csOracle": {
    "prefix": "csOracle",
    "description": "",
    "body": [
      "\"${1:Default}\": \"Data Source=${2:localhost}/${3:ORCL};User ID=${4:USERNAME};password=${5:PASSWORD}\",$0"
    ]
  }
}
